<BorderPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="palokunnanKoulutusrekisteri.TyontekijaDialogController" >
   <bottom>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label prefWidth="350.0" />
            <HBox />
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="buttonTallenna" defaultButton="true" mnemonicParsing="false" text="OK" onAction="#handleDefaultOK" />
                  <Button cancelButton="true" mnemonicParsing="false" text="Cancel" onAction="#handleDefaultCancel" />
              </buttons>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </ButtonBar>
         </children>
      </VBox>
   </bottom>
   <center>
      <ScrollPane fitToWidth="true" pannable="true" BorderPane.alignment="CENTER">
         <content>
            <GridPane hgap="10.0">
               <columnConstraints>
                  <ColumnConstraints fillWidth="false" halignment="RIGHT" hgrow="NEVER" minWidth="10.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label text="Nimi:" />
                  <Label text="Työntekijätunnus:" GridPane.rowIndex="1" />
                  <Label text="Tehtäväalue:" GridPane.rowIndex="2" />
                  <Label text="Virka-asema:" GridPane.rowIndex="3" />
                  <TextField prefWidth="200.0" maxWidth="200.0" text="Ankka Aku" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
                  <TextField prefWidth="200.0" maxWidth="200.0" text="1" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
                  <ComboBoxChooser prefWidth="200.0" maxWidth="200.0" rivit="_&#10;Ensihoito&#10;Riskienhallinta&#10;Pelastus&#10;Hallinto&#10;" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                  <ComboBoxChooser prefWidth="200.0" maxWidth="200.0" rivit="_&#10;Palomies&#10;Paloesimies&#10;Palomestari&#10;Palotarkastaja&#10;Palopäällikkö&#10;Toimistosihteeri&#10;Tilannekeskuspäivystäjä&#10;" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
               </children>
               <opaqueInsets>
                  <Insets left="10.0" top="10.0" />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </padding>
            </GridPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
