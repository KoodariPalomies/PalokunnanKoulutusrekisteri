<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" prefHeight="295.0" prefWidth="468.0" fx:controller="palokunnanKoulutusrekisteri.AloitusIkkunaGUIController">
	<center>
		<VBox alignment="CENTER"  BorderPane.alignment="CENTER">
			<children>
				<Label alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" prefHeight="50.0" text="PALOKUNNANKOULUTUSREKISTERI">
					<font>
					<Font name="Arial Bold" size="24.0"/>
					</font>
				</Label>
			</children>
				<children>
					<Label alignment="CENTER" prefHeight="20.0" text="Kuva tähän">
					</Label> 
				</children>
		</VBox>
	</center>
<bottom>
	<VBox BorderPane.alignment="CENTER">
		<children>
			<HBox>
		<children>
		<GridPane hgap="10.0" vgap="10.0">
			<Label text="Käyttäjätunnus" GridPane.rowIndex="1" />
				<padding>
					<Insets left="10.0" right="10.0" top="10.0"/>
				</padding>
				<TextField fx:id="textVastaus" text="" HBox.hgrow="ALWAYS" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
					<padding>
					<Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
					</padding>
						<HBox.margin>
						<Insets right="10.0" top="5.0"/>
						</HBox.margin>
						<Label text="Salasana" GridPane.rowIndex="2" />
						<padding>
							<Insets left="10.0" right="10.0" top="10.0"/>
							</padding>
							<PasswordField fx:id="textVastaus" text="" HBox.hgrow="ALWAYS" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
								<padding>
								<Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
								</padding>
									<HBox.margin>
									<Insets left="10.0" right="10.0" top="5.0"/>
									</HBox.margin>
						 </GridPane>
		</children>
			</HBox>
				<ButtonBar prefHeight="40.0" prefWidth="200.0">
					<buttons>
					<Button defaultButton="true" mnemonicParsing="false" onAction="#handleOK" text="Kirjaudu"/>
					<Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" text="Cancel"/>
					</buttons>
						<padding>
						<Insets right="10.0"/>
						</padding>
				</ButtonBar>
		</children>
	</VBox>
</bottom>
</BorderPane>