<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="497.0" prefWidth="354.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="palokunnanKoulutusrekisteri.TulostusController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="buttonTulosta" defaultButton="true" mnemonicParsing="false" text="Tulosta" onAction="#handleTulosta" />
          <Button fx:id="buttonSulje" cancelButton="true" mnemonicParsing="false" text="Cancel" onAction="#handleDefaultCancel" />
        </buttons>
         <padding>
            <Insets right="10.0" />
         </padding>
      </ButtonBar>
   </bottom>
   <center>
      <TextArea fx:id="tulostusAlue" prefHeight="200.0" prefWidth="200.0" text="Valitut työntekijät&#10;" BorderPane.alignment="CENTER">
	</TextArea>
   </center>
</BorderPane>
